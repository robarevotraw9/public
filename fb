#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "请输入斐波那契数列的项数: ";
    cin >> n;

    int a = 0, b = 1, c;
    cout << "斐波那契数列: " << a << " " << b << " ";

    for (int i = 2; i < n; i++) {
        c = a + b;
        cout << c << " ";
        a = b;
        b = c;
    }
    cout << endl;

    return 0;
}
